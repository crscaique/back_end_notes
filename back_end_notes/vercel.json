{
  "framework":null,

  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "wsgi.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS, DELETE, PUT"
      }
    },
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ]
}