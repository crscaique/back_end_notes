{
  "version": 2,
  "builds": [
    {
      "src": "back_end_notes/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "back_end_notes/wsgi.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS, DELETE, PUT"
      }
    },
    {
      "src": "/(.*)",
      "dest": "back_end_notes/wsgi.py"
    }
  ]
}